  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"npm run watch-client\" \"npm run watch-server\" \"npm run dev-server\"",
    "build": "concurrently \"npm run build-client\" \"npm run build-server\"",
    "dev-server": "nodemon -i ./src/server.js ./build/server.js",
    "build-client": "webpack",
    "build-server": "webpack --config ./.webpack/webpack.config.server.js",
    "watch-client": "webpack -w",
    "watch-server": "webpack --config ./.webpack/webpack.config.server.js -w"
  },
