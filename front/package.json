    "babel-loader": "^9.1.2",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "http-proxy-middleware": "^2.0.6",
    "nodemon": "^2.0.22",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
